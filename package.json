    "fs": "0.0.1-security",
    "googleapis": "^27.0.0",
    "material-ui": "^0.20.0",
    "material-ui-number-input": "^5.0.24",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-big-calendar": "^0.17.0",
    "react-clock": "^2.2.0",
    "react-color": "^2.13.8",
    "react-dom": "^16.2.0",
    "react-live-clock": "^2.0.2",
